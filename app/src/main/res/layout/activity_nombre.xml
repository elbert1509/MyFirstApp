<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nombreActivity">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="363dp"
        android:layout_height="101dp"
        android:layout_marginTop="56dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imageButton01"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chidd1"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <ImageButton
            android:id="@+id/imageButton02"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre2" />

        <ImageButton
            android:id="@+id/imageButton03"
            android:layout_width="100dp"
            android:layout_height="82dp"
            android:layout_weight="0.5"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre3" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="362dp"
        android:layout_height="107dp"
        android:layout_marginTop="72dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <ImageButton
            android:id="@+id/imageButton04"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre4" />

        <ImageButton
            android:id="@+id/imageButton05"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre5" />

        <ImageButton
            android:id="@+id/imageButton06"
            android:layout_width="100dp"
            android:layout_height="82dp"
            android:layout_weight="0.5"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre6" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="380dp"
        android:layout_height="119dp"
        android:layout_marginTop="84dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">


        <ImageButton
            android:id="@+id/imageButton07"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre7" />

        <ImageButton
            android:id="@+id/imageButton08"
            android:layout_width="100dp"
            android:layout_height="81dp"
            android:layout_weight="0"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre8" />

        <ImageButton
            android:id="@+id/imageButton09"
            android:layout_width="100dp"
            android:layout_height="82dp"
            android:layout_weight="0.5"
            android:background="#00FFFFFF"
            android:contentDescription="@string/desc"
            app:srcCompat="@drawable/chiffre9" />


    </LinearLayout>

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="161dp"
        android:layout_height="90dp"
        android:layout_marginTop="56dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        android:contentDescription="@string/desc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chiffre10" />
</androidx.constraintlayout.widget.ConstraintLayout>